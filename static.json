{
  "root": "desapp-grouph-frontend/dist",
  "clean_urls": true,
  "proxies": {
    "/api/": {
      "origin": "https://unq-des-app.herokuapp.com/rest"
    }
  },
    "headers": {
    "/**": {
      "Access-Control-Allow-Origin": "*"
    },
  "**.js": {
    "Cache-Control": "public, max-age=31536000",
    "Access-Control-Allow-Origin": "*"
  },
  "**.css": {
    "Cache-Control": "public, max-age=31536000",
    "Access-Control-Allow-Origin": "*"
  }
  }
}
